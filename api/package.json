{
	"name": "api",
	"version": "1.0.0",
	"scripts": {
		"prebuild": "rimraf dist",
		"build:prisma": "npx prisma generate && npx prisma db push",
		"build:api": "nest build",
		"build": "npm run build:prisma && npm run build:api",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
